class Student {
    String name;
    int age;
    Student(String studentName, int studentAge) {
        name = studentName;
        age = studentAge;
        System.out.println("Constructor called for " + name);
    }
    void displayInfo() {
        System.out.println("Name: " + name + ", Age: " + age);
    }
}

public class Main {
    public static void main(String[] args) {
        Student s1 = new Student("Alice", 18);
        s1.displayInfo();
        System.out.println(); 
        Student s2 = new Student("Bob", 20);
        s2.displayInfo();
        System.out.println();
        Student s3 = new Student("Charlie", 19);
        s3.displayInfo();
    }
}
